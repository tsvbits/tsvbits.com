{
  "name": "tsvbits",
  "private": true,
  "description": "tsvbits",
  "version": "1.0.0",
  "author": "Sergey Todyshev <stodyshev@gmail.com>",
  "bugs": {
    "url": "https://github.com/tsvbits/tsvbits.github.io/issues"
  },
  "dependencies": {
    "gatsby": "^2.19.49",
    "gatsby-plugin-catch-links": "^2.1.28",
    "gatsby-plugin-feed": "^2.3.29",
    "gatsby-plugin-google-analytics": "^2.1.38",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.48",
    "gatsby-plugin-offline": "^3.0.42",
    "gatsby-plugin-react-helmet": "^3.1.24",
    "gatsby-plugin-sharp": "^2.4.13",
    "gatsby-plugin-typography": "^2.3.25",
    "gatsby-remark-autolink-headers": "2.1.26",
    "gatsby-remark-copy-linked-files": "^2.1.40",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.50",
    "gatsby-remark-prismjs": "^3.3.36",
    "gatsby-remark-responsive-iframe": "^2.2.34",
    "gatsby-remark-smartypants": "^2.1.23",
    "gatsby-source-filesystem": "^2.1.57",
    "gatsby-transformer-remark": "^2.6.59",
    "gatsby-transformer-sharp": "^2.3.19",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "prismjs": "^1.19.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "2.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  },
  "homepage": "https://tsvbits.com",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "build": "gatsby build && echo \"tsvbits.com\" >> public/CNAME && echo \"\" >> public/.nojekyll",
    "rebuild": "npm run clean && npm run build",
    "predeploy": "npm run rebuild",
    "deploy": "gh-pages --dist public --branch master --add --dotfiles",
    "dry": "yarn build && cp now.json public/ && cd public && now"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  }
}
