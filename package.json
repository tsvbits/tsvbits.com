{
  "name": "tsvbits",
  "private": true,
  "description": "tsvbits",
  "version": "1.0.0",
  "author": "Sergey Todyshev <stodyshev@gmail.com>",
  "bugs": {
    "url": "https://github.com/tsvbits/tsvbits.com/issues"
  },
  "dependencies": {
    "gatsby": "^2.13.57",
    "gatsby-plugin-catch-links": "^2.1.2",
    "gatsby-plugin-feed": "^2.3.6",
    "gatsby-plugin-google-analytics": "^2.1.7",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.5",
    "gatsby-plugin-offline": "^2.2.5",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-sharp": "^2.2.11",
    "gatsby-plugin-typography": "^2.3.2",
    "gatsby-remark-autolink-headers": "2.1.3",
    "gatsby-remark-copy-linked-files": "^2.1.5",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.8",
    "gatsby-remark-prismjs": "^3.3.5",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-remark-smartypants": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.9",
    "gatsby-transformer-remark": "^2.6.12",
    "gatsby-transformer-sharp": "^2.2.6",
    "lodash": "^4.17.15",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.14.2"
  },
  "homepage": "https://tsvbits.com",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "build": "gatsby build && echo \"tsvbits.com\" >> public/CNAME && cp travis-jekyll.yml public/.travis.yml",
    "rebuild": "npm run clean && npm run build",
    "predeploy": "npm run rebuild",
    "deploy": "gh-pages --dist public --branch master --add --dotfiles",
    "dry": "yarn build && cp now.json public/ && cd public && now"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  }
}
