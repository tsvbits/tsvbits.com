  "bugs": {
    "url": "https://github.com/tsvbits/tsvbits.github.io/issues"
  },
  "dependencies": {
    "@iconify/icons-ant-design": "^1.0.8",
    "@iconify/icons-uil": "^1.0.14",
    "@iconify/react": "^1.1.3",
    "@loadable/component": "^5.13.1",
    "gatsby": "^2.24.57",
    "gatsby-plugin-catch-links": "^2.3.11",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.28",
    "gatsby-plugin-offline": "^3.2.27",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.35",
    "gatsby-plugin-typography": "^2.5.10",
    "gatsby-remark-autolink-headers": "2.3.12",
    "gatsby-remark-copy-linked-files": "^2.3.14",
    "gatsby-remark-emoji": "^0.0.3",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.3.29",
    "gatsby-remark-prismjs": "^3.5.11",
    "gatsby-remark-responsive-iframe": "^2.4.13",
    "gatsby-remark-smartypants": "^2.3.10",
    "gatsby-source-filesystem": "^2.3.29",
    "gatsby-transformer-remark": "^2.8.34",
    "gatsby-transformer-sharp": "^2.5.14",
    "lodash": "^4.17.20",
    "moment": "^2.28.0",
    "node-sass": "^4.14.1",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-content-loader": "^5.1.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-text-loop": "^2.3.0",
    "react-typography": "^0.16.19",
    "simple-icons": "^3.7.0",
    "swr": "^0.3.2",
    "typeface-jetbrains-mono": "^2.0.0",
    "typography": "^0.16.19",
    "typography-theme-moraga": "^0.16.19",
    "vanilla-back-to-top": "^7.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "eslint-plugin-react": "^7.20.6",
    "gh-pages": "3.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1"
  },
  "homepage": "https://tsvbits.com",
  "license": "CC",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "build": "gatsby build && echo \"tsvbits.com\" >> public/CNAME && echo \"\" >> public/.nojekyll",
    "rebuild": "npm run clean && npm run build",
    "predeploy": "npm run rebuild",
    "deploy": "gh-pages --dist public --branch master --add --dotfiles",
    "dry": "yarn build && cp now.json public/ && cd public && now",
    "fmt": "prettier --write '**/*.*'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  }
}
