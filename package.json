{
  "name": "tsvbits",
  "private": true,
  "description": "tsvbits",
  "version": "1.0.0",
  "author": "Sergey Todyshev <stodyshev@gmail.com>",
  "bugs": {
    "url": "https://github.com/tsvbits/tsvbits.github.io/issues"
  },
  "dependencies": {
    "@iconify/icons-uil": "^1.0.13",
    "@iconify/react": "^1.1.3",
    "gatsby": "^2.24.4",
    "gatsby-plugin-catch-links": "^2.3.11",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-google-analytics": "^2.3.11",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-offline": "^3.2.18",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.19",
    "gatsby-plugin-typography": "^2.5.10",
    "gatsby-remark-autolink-headers": "2.3.11",
    "gatsby-remark-copy-linked-files": "^2.3.11",
    "gatsby-remark-emoji": "^0.0.3",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.3.19",
    "gatsby-remark-prismjs": "^3.5.10",
    "gatsby-remark-responsive-iframe": "^2.4.11",
    "gatsby-remark-smartypants": "^2.3.10",
    "gatsby-source-filesystem": "^2.3.20",
    "gatsby-transformer-remark": "^2.8.25",
    "gatsby-transformer-sharp": "^2.5.11",
    "lodash": "^4.17.19",
    "node-sass": "^4.14.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-text-loop": "^2.3.0",
    "react-typography": "^0.16.19",
    "simple-icons": "^3.2.0",
    "typography": "^0.16.19",
    "typography-theme-moraga": "^0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-react": "^7.20.3",
    "gh-pages": "3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "homepage": "https://tsvbits.com",
  "license": "CC",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "build": "gatsby build && echo \"tsvbits.com\" >> public/CNAME && echo \"\" >> public/.nojekyll",
    "rebuild": "npm run clean && npm run build",
    "predeploy": "npm run rebuild",
    "deploy": "gh-pages --dist public --branch master --add --dotfiles",
    "dry": "yarn build && cp now.json public/ && cd public && now",
    "fmt": "prettier --write '**/*.*'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  }
}
